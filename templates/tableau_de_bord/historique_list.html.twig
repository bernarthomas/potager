{% else %}

<table>
    <tr><th>date</th><th>action</th><th>entite</th><th>occurence</th><th>utilisateur</th></tr>
    {% for valeur in historique %}
    <tr>
        <td>{{valeur.date|date("d/m/Y H:i:s")}}</td>
        <td>{{valeur.action.libelle}}</td>
        <td>{{valeur.entite.libelle}}</td>
        <td>{{valeur.occurenceId}}</td>
        <td>{{valeur.utilisateur.email}}</td>
    </tr>
    {% endfor %}
</table>
{% endif %}